// C program to divide two numbers, print the quotient and remainder â€“ without the division operation ?

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 int n,num1,num2,temp,count,m;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        scanf("%d",&num1);
        temp=num1;
        count=0;
        scanf("%d",&num2);
            while(num1>=num2){
                num1=num1-num2;
                count=count+1;
            }
        //Quotient
        printf("%d / %d = %d\n",temp,num2,count);
        //Remainder
        printf("%d %% %d = %d\n",temp,num2,num1);
    }
    return 0;
}
